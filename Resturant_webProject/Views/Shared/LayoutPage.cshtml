<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
  
    <link href="~/assests/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="~/assests/css/style.css" rel="stylesheet" type="text/css">
    <link href="~/assests/css/flexslider.css" rel="stylesheet" type="text/css">
    <link href="~/assests/icons/css/ionicons.min.css" rel="stylesheet" type="text/css">
    <link href="~/assests/icons/css/simple-line-icons.css" rel="stylesheet" type="text/css">

    <!--revolution slider setting css-->
    <link href="~/assests/rs-plugin/css/settings.css" rel="stylesheet">
    <link href="~/assests/css/prettyPhoto.css" rel="stylesheet" type="text/css" />
   
</head>
<body  data-target=".navbar" data-offset="80">
    <nav class="navbar navbar-default navbar-fixed-top before-color">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand alo" href="index.html">Restaurant</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right scroll-to">
                    <li class="active"><a href='@Url.Action("Index", "Home", new {})'>Home</a></li>
                    <li><a href='@Url.Action("About", "Home", new {})'>About</a></li>
                    <li><a href='@Url.Action("Contact", "Home", new {})'>Contact</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button">Staff <span class="caret"></span></a>
                        <ul class="dropdown-menu" role="menu">
                            <li><a href='@Url.Action("Index", "Staffs", new {})'>All Staff</a></li>
                            <li><a href='@Url.Action("Create", "Staffs", new {})'>Add Staff</a></li>

                        </ul>

                    </li>
                    <li><a href='@Url.Action("Login2", "Staffs", new {})'>LogOut</a></li>


                </ul>
            </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
    </nav>
    <div class="fullwidthbanner" id="home">
        <div class="tp-banner">
            <ul>
                <!-- SLIDE 1 -->
                <li data-transition="fade" data-slotamount="7" data-masterspeed="1500">
                    <!-- MAIN IMAGE -->
                    <img src="~/assests/images/DailyPlanet_3x2.jpg" alt="desk" data-bgfit="cover" data-bgposition="left top" data-bgrepeat="no-repeat">
                    <!-- LAYERS -->
                    <!-- LAYER NR. 1 -->
                    <div class="tp-caption slider-title" data-x="center" data-y="center" data-voffset="-30" data-speed="500" data-start="1200" data-easing="Power4.easeInOut">
                        welcome to <span>OUR Reaturant</span>
                    </div> <!-- /tp-caption -->
                    <!-- LAYER NR. 2 -->
                    <div class="tp-caption slider-caption" data-x="center" data-y="center" data-voffset="40" data-speed="500" data-start="1800" data-easing="Power4.easeInOut" data-captionhidden="on" style="color:white">
                        we will provide you the best service
                    </div> <!-- /tp-caption -->
                    <!-- LAYER NR. 3 -->
                    <div class="tp-caption slider-button scroll-to" data-x="center" data-y="center" data-voffset="120" data-speed="500" data-start="2400" data-easing="Power4.easeInOut" data-captionhidden="on">
                        <a class="btn btn-white" href="#about" style="color:black">See more</a>
                    </div> <!-- /tp-caption -->
                </li>

            </ul>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>
    <footer class="footer" style="background-color: #6A371B">
        <div class="container text-center">
            <span class="alo">Retaurant</span>
            <ul class="social list-inline">
                <li><a href="#"><i class="icon icon-social-twitter"></i></a></li>
                <li><a href="#"><i class="icon icon-social-facebook"></i></a></li>
                <li><a href="#"><i class="icon icon-social-dribbble"></i></a></li>
            </ul>

        </div>
    </footer>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="~/assests/js/jquery.min.js" type="text/javascript"></script>
    <script src="~/assests/js/moderniz.min.js" type="text/javascript"></script>
    <script src="~/assests/js/jquery.easing.1.3.js" type="text/javascript"></script>
    <!-- bootstrap js-->
    <script src="~/assests/bootstrap/js/bootstrap.min.js" type="text/javascript"></script>
    <script type="text/javascript" src="~/assests/js/jquery.flexslider-min.js"></script>
    <script type="text/javascript" src="~/assests/js/parallax.min.js"></script>
    <script type="text/javascript" src="~/assests/js/jquery.prettyPhoto.js"></script>
    <script type="text/javascript" src="~/assests/js/jqBootstrapValidation.js"></script>
    <!--revolution slider scripts-->
    <script src="~/assests/rs-plugin/js/jquery.themepunch.tools.min.js" type="text/javascript"></script>
    <script src="~/assests/rs-plugin/js/jquery.themepunch.revolution.min.js" type="text/javascript"></script>
    <script src="~/assests/js/template.js" type="text/javascript"></script>

</body>
</html>


